<Window x:Class="CsWebChat.WpfClient.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CsWebChat.WpfClient"
        mc:Ignorable="d"
        Title="CsWebChat" Height="450" Width="800"
        
        xmlns:regions="clr-namespace:CsWebChat.WpfClient.Regions;assembly=CsWebChat.WpfClient.Regions"
        
        xmlns:prism="http://prismlibrary.com/"
        prism:ViewModelLocator.AutoWireViewModel="True">
    <UserControl prism:RegionManager.RegionName="{Binding Source={x:Static regions:RegionNames.MAIN_REGION}}">
        
    </UserControl>
</Window>
